pipeline{
    agent {label "sasmachine"}
    stages{
        stage('prepare'){
            steps{
                sh "echo helloworld"
            }
        stage('New Code'){
            steps{
             echo "new code commited"
             
            }
        }
        stage('Checkout to Production'){
            steps{
             echo "Tag name ${ref}"
            }

        }
        
        }
    } 
}   
