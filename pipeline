pipeline{

    agent {label "sasmachine"}
    stages{
        stage('prepare'){
            steps{
                sh "echo helloworld"
            }
        }
        stage('New Code'){
            steps{
             echo "new code commited"
             
            }
        }
        stage('Checkout to Production'){
            when {
                expression{
                    ${ref}  == "*trunk"
                }
            }
            steps{
             echo "Tag name ${tagID}"
            }

        }

        
    } 
}   
