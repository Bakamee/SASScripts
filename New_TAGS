pipeline{

    agent {label "sasmachine"}
    environment{
    tag = "${ref}"
    var="null"
    }
    stages{
        stage(prepare){
           sh "echo ${tag##*/}"
        }
        
        stage ("New Code Developed"){
            steps{
                echo "new dev code commited, needs to be productionised"
                 sh "echo ${tag}"
            }
        }
        stage("Checkout  to Prod"){
            steps {
             echo "checkout steps"
            }
        }
        
    }
}
